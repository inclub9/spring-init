type Team {
    id: ID!
    name: String!
}

type Query {
    allTeams: [Team]
    team(id: ID!): Team
}

type Mutation {
    createTeam(name: String!): Team!
    deleteTeam(id: ID!): [Team]
    updateTeamName(id: ID!,name: String!): Int!
    addMembersToTeam(teamId: ID!, members: [MemberInput]!): [Member]!
    removeMembersInTeam(memberIds: [ID]!):[Member]!
}